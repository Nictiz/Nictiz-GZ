    <description>Creation of wiki document(s) for an application/version.</description>

    <command-line-options>
        <xi:include href="../../../../YATC-shared/chtmod/help-command-line-option.xml"/>
        <xi:include href="../../../../YATC-shared/chtmod/list-command-line-option.xml"/>
        
        <command-line-option >
            <flag name="process" optional="true"/>
            <flag name="nosetup" optional="true">Do not process the setup elements.</flag>
            <flag name="refresh" optional="true">Refresh the source data by first running `get-production-ada-instances`.</flag>
            <argument name="application"/>
            <argument name="version"/>
            <description>Process the application/version combinations available. To process all applications/versions available, use either `#all` or `%all` for the argument(s).</description>
        </command-line-option>
        
    </command-line-options>
    
</command-help-text>
