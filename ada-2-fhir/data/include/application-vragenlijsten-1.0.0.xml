    <!-- beschikbaarstellen_vragenlijst -->

    <setup usecase="beschikbaarstellen_vragenlijst" directory-id="bsvl">
        <copy-project-schemas>
            <include glob="*$USECASE.xsd"/>
        </copy-project-schemas>
        <copy-directory directory="#../../env/$BASEPATH/ada_instance/" target-subdir="ada_instance" directory-id="bsvl-ada-instance"/>
        <empty-directory target-subdir="fhir_instance" directory-id="bsvl-fhir-instance"/>
    </setup>

    <build name="build-beschikbaarstellen_vragenlijst-fhir">
        <stylesheet href="../../env/$BASEPATH/beschikbaarstellen_vragenlijst/payload/beschikbaarstellen_vragenlijst_2_fhir_resources.xsl"/>
        <input-documents directory="@bsvl-ada-instance"/>
        <discard-output/>
        <parameter name="bundleSelfLink" value="http://dummy.nictiz.nl/dummyquery"/>
        <parameter name="outputDirectory" directory="@bsvl-fhir-instance"/>
    </build>
    <build name="build-beschikbaarstellen_vragenlijst-fhir-resources">
        <stylesheet href="../../env/$BASEPATH/beschikbaarstellen_vragenlijst/payload/beschikbaarstellen_vragenlijst_2_fhir.xsl"/>
        <input-documents directory="@bsvl-ada-instance"/>
        <output-documents directory="@bsvl-fhir-instance"/>
        <parameter name="bundleSelfLink" value="http://dummy.nictiz.nl/dummyquery"/>
    </build>

